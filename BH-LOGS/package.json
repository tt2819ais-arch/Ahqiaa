# Минимальный package.json
cat > package.json << 'EOF'
{
  "name": "bh-logs",
  "scripts": {
    "build": "echo 'Building...' && mkdir -p dist && cp *.html dist/ 2>/dev/null || true"
  }
}
EOF

# Просто создай папку dist с index.html
mkdir -p dist
cat > dist/index.html << 'EOF'
<!DOCTYPE html>
<html>
<head><title>BH-LOGS</title></head>
<body><h1>BH-LOGS System</h1></body>
</html>
EOF
